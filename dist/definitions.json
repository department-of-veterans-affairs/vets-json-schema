{
  "fullName": {
    "type": "object",
    "properties": {
      "first": {
        "type": "string",
        "minLength": 1,
        "maxLength": 30
      },
      "middle": {
        "type": "string"
      },
      "last": {
        "type": "string",
        "minLength": 1,
        "maxLength": 30
      },
      "suffix": {
        "type": "string",
        "enum": [
          "Jr.",
          "Sr.",
          "II",
          "III",
          "IV"
        ]
      }
    },
    "required": [
      "first",
      "last"
    ]
  },
