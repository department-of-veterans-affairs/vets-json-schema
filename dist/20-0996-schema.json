{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "HIGHER-LEVEL REVIEW",
  "type": "object",
  "properties": {
    "data": {
      "type": "object",
      "properties": {
        "type": {
          "type": "string",
          "enum": [
            "HigherLevelReview"
          ],
          "enumNames": [
            "HLR"
          ]
        },
        "attributes": {
          "type": "object",
          "required": [
            "benefitType",
            "veteran",
            "sameOffice",
            "informalConference"
          ],
          "benefitType": {
            "type": "string",
            "enum": [
              "compensation"
            ],
            "enumNames": [
              "Compensation"
            ]
          },
          "veteran": {
            "type": "object",
            "required": [
              "address"
            ],
            "properties": {
              "address": {
                "type": "object",
                "required": [
                  "zipCode5"
                ],
                "properties": {
                  "zipCode5": {
                    "type": "string",
                    "pattern": "^[0-9]{5}$"
                  }
                }
              },
              "phone": {
                "type": "object",
                "properties": {
                  "areaCode": {
                    "type": "string",
                    "pattern": "^[2-9][0-9]{2}$"
                  },
                  "phoneNumber": {
                    "type": "string",
                    "pattern": "^[0-9]{1,14}$"
                  },
                  "phoneNumberCountryCode": {
                    "type": "string",
                    "pattern": "^[0-9]+$"
                  },
                  "phoneNumberExt": {
                    "type": "string",
                    "pattern": "^[a-zA-Z0-9]{1,10}$"
                  }
                }
              },
              "emailAddressText": {
                "type": "string",
                "format": "email"
              },
              "timeZone": {
                "type": "string"
              }
            }
          },
          "sameOffice": {
            "type": "boolean"
          },
          "informalConference": {
            "type": "boolean"
          },
          "informalConferenceRep": {
            "type": "object",
            "properties": {
              "name": {
                "type": "string"
              },
              "phone": {
                "type": "object",
                "properties": {
                  "areaCode": {
                    "type": "string",
                    "pattern": "^[2-9][0-9]{2}$"
                  },
                  "phoneNumber": {
                    "type": "string",
                    "pattern": "^[0-9]{1,14}$"
                  },
                  "phoneNumberCountryCode": {
                    "type": "string",
                    "pattern": "^[0-9]+$"
                  },
                  "phoneNumberExt": {
                    "type": "string",
                    "pattern": "^[a-zA-Z0-9]{1,10}$"
                  }
                }
              }
            }
          },
          "informalConferenceTimes": {
            "type": "array",
            "minItems": 1,
            "maxItems": 2,
            "uniqueItems": true,
            "items": {
              "type": "string",
              "enum": [
                "800-1000 ET",
                "1000-1230 ET",
                "1230-1400 ET",
                "1400-1630 ET"
              ]
            }
          }
        }
      }
    },
    "included": {
      "type": "array",
      "minItems": 1,
      "maxItems": 100,
      "items": {
        "type": "object",
        "required": [
          "type"
        ],
        "properties": {
          "type": {
            "type": "string",
            "enum": [
              "ContestableIssue"
            ]
          },
          "attributes": {
            "type": "object",
            "required": [
              "issue",
              "decisionDate"
            ],
            "properties": {
              "issue": {
                "type": "string",
                "maxLength": 140
              },
              "decisionDate": {
                "type": "string",
                "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$"
              },
              "decisionIssueId": {
                "type": "number"
              },
              "ratingIssueId": {
                "type": "string"
              },
              "ratingDecisionIssueId": {
                "type": "string"
              }
            }
          }
        }
      }
    }
  }
}
