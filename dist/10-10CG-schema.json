    "phone": {
      "type": "string",
      "minLength": 10
    },
    "email": {
      "type": "string",
      "minLength": 6,
      "maxLength": 80,
      "pattern": "^[_A-Za-z0-9-]+(\\.[_A-Za-z0-9-]+)*@[A-Za-z0-9-]+(\\.[A-Za-z0-9]+)*(\\.[A-Za-z]{2,})$"
