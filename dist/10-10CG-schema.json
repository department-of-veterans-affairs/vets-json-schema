        "postalCode": {
          "type": "string",
          "pattern": "^(\\d{5})(?:[-](\\d{4}))?$"
        }
      }
    },
    "vetRelationship": {
      "type": "string",
      "enum": [
        "Spouse",
        "Father",
        "Mother",
        "Son",
        "Daughter",
        "Brother",
        "Sister",
        "Significant Other",
        "Relative - Other",
        "Friend/Neighbor",
        "Grandchild"
      ]
    }
  },
