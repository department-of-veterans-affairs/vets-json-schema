{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Application for Comprehensive Assistance for Family Caregivers Program (10-10CG)",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "veteran",
    "primaryCaregiver"
  ],
  "definitions": {
    "fullName": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "first",
        "last"
      ],
      "properties": {
        "first": {
          "type": "string",
          "minLength": 1,
          "maxLength": 30
        },
        "middle": {
          "type": "string"
        },
        "last": {
          "type": "string",
          "minLength": 1,
          "maxLength": 30
        }
      }
    },
    "ssn": {
      "type": "string",
      "pattern": "^[0-9]{9}$"
    },
    "date": {
      "pattern": "^(\\d{4}|XXXX)-(0[1-9]|1[0-2]|XX)-(0[1-9]|[1-2][0-9]|3[0-1]|XX)$",
      "type": "string"
    },
    "gender": {
      "type": "string",
      "enum": [
        "F",
        "M"
      ]
    },
    "phone": {
      "type": "string",
      "minLength": 10
    },
    "email": {
      "type": "string",
      "maxLength": 256,
      "format": "email"
    },
    "address": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "street",
        "city",
        "state",
        "postalCode"
      ],
