{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "SUPPLEMENTAL CLAIM FOR COMPENSATION (21-526EZ)",
  "type": "object",
  "definitions": {
    "directDeposit": {
      "type": "object",
      "properties": {
        "accountType": {
          "type": "string",
          "enum": [
            "checking",
            "savings"
          ]
        },
        "routingNumber": {
          "type": "string",
          "pattern": "^\\d{9}$"
        },
        "accountNumber": {
          "type": "string"
        },
        "bankName": {
          "type": "string"
        }
      }
    },
    "date": {
      "pattern": "^(\\d{4}|XXXX)-(0[1-9]|1[0-2]|XX)-(0[1-9]|[1-2][0-9]|3[0-1]|XX)$",
      "type": "string"
    },
    "phone": {
      "type": "string",
      "minLength": 10
    },
    "fullPhone": {
      "type": "object",
      "properties": {
        "areaNumber": {
          "type": "string"
        },
        "phoneNumber": {
          "$ref": "#/definitions/phone"
        }
      }
    },
    "fullName": {
      "type": "object",
      "properties": {
        "first": {
          "type": "string",
          "minLength": 1,
          "maxLength": 30
        },
        "middle": {
          "type": "string"
        },
        "last": {
          "type": "string",
          "minLength": 1,
          "maxLength": 30
        },
        "suffix": {
          "type": "string",
          "enum": [
            "Jr.",
            "Sr.",
            "II",
            "III",
            "IV"
          ]
        }
      },
      "required": [
        "first",
        "last"
      ]
    }
  },
  "properties": {
    "veteran": {
      "type": "object",
      "properties": {
        "emailAddress": {
          "type": "string",
          "format": "email"
        },
        "alternateEmailAddress": {
          "type": "string",
          "format": "email"
        },
        "mailingAddress": {
          "type": "object",
          "oneOf": [
            {
              "properties": {
                "country": {
                  "not": {
                    "type": "string",
                    "enum": []
                  }
                },
                "state": {
                  "type": "string",
                  "maxLength": 51
                }
              }
            }
          ],
          "properties": {
            "addressLine1": {
              "type": "string"
            },
            "addressLine2": {
              "type": "string"
            },
            "addressLine3": {
              "type": "string"
            },
            "city": {
              "type": "string"
            },
            "zipFirstFive": {
              "type": "number",
              "minLength": 5,
              "maxLength": 5
            },
            "zipLastFour": {
              "type": "number",
              "minLength": 4,
              "maxLength": 4
            },
            "militaryPostOfficeTypeCode": {
              "type": "string"
            },
            "militaryStateCode": {
              "type": "string"
            },
            "type": {
              "type": "string",
              "enum": [
                "international",
                "domestic",
                "military"
              ]
            }
          }
        },
        "primaryPhone": {
          "$ref": "#/definitions/fullPhone"
        },
        "forwardingAddress": {
          "type": "object",
          "oneOf": [
            {
              "properties": {
                "country": {
                  "not": {
                    "type": "string",
                    "enum": []
                  }
                },
                "state": {
                  "type": "string",
                  "maxLength": 51
                }
              }
            }
          ],
          "properties": {
            "addressLine1": {
              "type": "string"
            },
            "addressLine2": {
              "type": "string"
            },
            "addressLine3": {
              "type": "string"
            },
            "city": {
              "type": "string"
            },
            "zipFirstFive": {
              "type": "number",
              "minLength": 5,
              "maxLength": 5
            },
            "zipLastFour": {
              "type": "number",
              "minLength": 4,
              "maxLength": 4
            },
            "militaryPostOfficeTypeCode": {
              "type": "string"
            },
            "militaryStateCode": {
              "type": "string"
            },
            "type": {
              "type": "string",
              "enum": [
                "international",
                "domestic",
                "military"
              ]
            },
            "efctvDt": {
              "$ref": "#/definitions/date"
            }
          }
        },
        "homelessness": {
          "type": "object",
          "properties": {
            "hasPointOfContact": {
              "type": "boolean"
            },
            "pointOfContact": {
              "type": "object",
              "properties": {
                "pointOfContactName": {
                  "type": "string"
                },
                "pointOfContactAreaCode": {
                  "type": "string"
                },
                "pointOfContactPhoneNumber": {
                  "type": "string"
                }
              }
            }
          }
        },
        "serviceNumber": {
          "type": "string"
        }
      }
    },
    "attachments": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "docName": {
            "type": "string"
          },
          "dateUploaded": {
            "type": "string"
          },
          "attachmentType": {
            "type": "string"
          },
          "inflightId": {
            "type": "string"
          }
        }
      }
    },
    "militaryPayments": {
      "type": "object",
      "properties": {
        "payments": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "amount": {
                "type": "number"
              },
              "payType": {
                "type": "string"
              }
            }
          }
        },
        "receiveCompensationInLieuOfRetired": {
          "type": "boolean"
        },
        "receivingInactiveDutyTrainingPay": {
          "type": "boolean"
        },
        "waveBenifitsToRecInactDutyTraiPay": {
          "type": "boolean"
        }
      }
    },
    "directDeposit": {
      "$ref": "#/definitions/directDeposit"
    },
    "serviceInformation": {
      "type": "object",
      "properties": {
        "servicePeriods": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "serviceBranch": {
                "type": "string"
              },
              "activeDutyBegin": {
                "$ref": "$/definitions/date"
              },
              "activeDutyEnd": {
                "$ref": "$/definitions/date"
              }
            }
          }
        },
        "servedInReservesOrNationalGuard": {
          "type": "boolean"
        },
        "obligationTermOfServiceFrom": {
          "type": "boolean"
        },
        "obligationTermOfServiceTo": {
          "type": "string"
        },
        "anticipatedSeparationDate": {
          "$ref": "$/definitions/date"
        },
        "servedInCombatZone": {
          "type": "boolean"
        },
        "currentlyActiveTitle10": {
          "type": "boolean"
        },
        "title10ActivationDate": {
          "$ref": "$/definitions/date"
        },
        "title10UnitName": {
          "type": "string"
        },
        "title10Phone": {
          "$ref": "#/definitions/fullPhone"
        },
        "separationLocationName": {
          "type": "string"
        },
        "separationLocationCode": {
          "type": "string"
        },
        "alternateNames": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/fullName"
          }
        },
        "confinements": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "confinementBegin": {
                "$ref": "#/definitions/date"
              },
              "confinementEnd": {
                "$ref": "#/definitions/date"
              },
              "verifiedInd": {
                "type": "boolean"
              }
            }
          }
        }
      }
    },
    "disabilities": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "disability": {
            "type": "object",
            "properties": {
              "diagnosticText": {
                "type": "string"
              },
              "decisionCode": {
                "type": "string"
              },
              "specialIssues": {
                "type": "object",
                "properties": {
                  "specialIssueCode": {
                    "type": "string"
                  },
                  "specialIssueName": {
                    "type": "string"
                  }
                }
              },
              "ratedDisabilityId": {
                "type": "string"
              },
              "disabilityActionType": {
                "type": "string"
              },
              "ratingDecisionId": {
                "type": "string"
              },
              "diagnosticCode": {
                "type": "string"
              },
              "secondaryDisabilities": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "diagnosticText": {
                      "type": "string"
                    },
                    "disabilityActionType": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "treatments": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "treatment": {
            "type": "object",
            "properties": {
              "treatmentCenterType": {
                "type": "string"
              },
              "startTreatment": {
                "$ref": "#/definitions/date"
              },
              "endTreatment": {
                "$ref": "#/definitions/date"
              },
              "treatmentCenterCountry": {
                "type": "string"
              },
              "treatmentCenterState": {
                "type": "string"
              },
              "treatmentCenterName": {
                "type": "string"
              },
              "treatmentCenterCity": {
                "type": "string"
              }
            }
          }
        }
      }
    },
    "specialCircumstances": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "specialCircumstance": {
            "type": "object",
            "properties": {
              "name": {
                "type": "string"
              },
              "code": {
                "type": "string"
              }
            }
          }
        }
      }
    }
  },
  "required": []
}
